<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>


<VBox prefWidth="400.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.illucit.partyinvoice.view.SaveConfirmController">
   <children>
      <Label text="%ui.saveconfirm.label" wrapText="true">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <HBox alignment="TOP_CENTER" spacing="10.0">
         <children>
            <Button defaultButton="true" mnemonicParsing="false" onAction="#save" text="%ui.saveconfirm.save" />
            <Button mnemonicParsing="false" onAction="#discard" text="%ui.saveconfirm.discard" />
            <Button cancelButton="true" mnemonicParsing="false" onAction="#cancel" text="%ui.saveconfirm.cancel" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
